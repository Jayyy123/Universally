<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/app_flow"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.jay.universally.presentation.ui.fragments.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_searchFragment"
            app:destination="@id/searchFragment" />
        <action
            android:id="@+id/action_homeFragment_to_settings2"
            app:destination="@id/settings2" />
        <action
            android:id="@+id/action_homeFragment_to_add2"
            app:destination="@id/add2" />
        <action
            android:id="@+id/action_homeFragment_to_about2"
            app:destination="@id/about2" />
        <action
            android:id="@+id/action_homeFragment_to_account"
            app:destination="@id/account" />
    </fragment>
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.jay.universally.presentation.ui.fragments.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" >
        <action
            android:id="@+id/action_searchFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_searchFragment_to_settings2"
            app:destination="@id/settings2" />
        <action
            android:id="@+id/action_searchFragment_to_add2"
            app:destination="@id/add2" />
        <action
            android:id="@+id/action_searchFragment_to_about2"
            app:destination="@id/about2" />
        <action
            android:id="@+id/action_searchFragment_to_account"
            app:destination="@id/account" />
    </fragment>
    <fragment
        android:id="@+id/settings2"
        android:name="com.jay.universally.presentation.ui.fragments.Settings"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings" >
        <action
            android:id="@+id/action_settings2_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_settings2_to_searchFragment"
            app:destination="@id/searchFragment" />
        <action
            android:id="@+id/action_settings2_to_about2"
            app:destination="@id/about2" />
        <action
            android:id="@+id/action_settings2_to_account"
            app:destination="@id/account" />
        <action
            android:id="@+id/action_settings2_to_searchFragment2"
            app:destination="@id/searchFragment" />
        <action
            android:id="@+id/action_settings2_to_add2"
            app:destination="@id/add2" />
    </fragment>
    <fragment
        android:id="@+id/add2"
        android:name="com.jay.universally.presentation.ui.fragments.Add"
        android:label="fragment_add"
        tools:layout="@layout/fragment_add" >
        <action
            android:id="@+id/action_add2_to_searchFragment"
            app:destination="@id/searchFragment" />
        <action
            android:id="@+id/action_add2_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/about2"
        android:name="com.jay.universally.presentation.ui.fragments.About"
        android:label="fragment_about"
        tools:layout="@layout/fragment_about" >
        <action
            android:id="@+id/action_about2_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_about2_to_settings2"
            app:destination="@id/settings2" />
        <action
            android:id="@+id/action_about2_to_account"
            app:destination="@id/account" />
        <action
            android:id="@+id/action_about2_to_searchFragment"
            app:destination="@id/searchFragment" />
        <action
            android:id="@+id/action_about2_to_add2"
            app:destination="@id/add2" />
    </fragment>
    <fragment
        android:id="@+id/account"
        android:name="com.jay.universally.presentation.ui.fragments.Account"
        android:label="fragment_account"
        tools:layout="@layout/fragment_account" >
        <action
            android:id="@+id/action_account_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_account_to_about2"
            app:destination="@id/about2" />
        <action
            android:id="@+id/action_account_to_searchFragment"
            app:destination="@id/searchFragment" />
        <action
            android:id="@+id/action_account_to_add2"
            app:destination="@id/add2" />
    </fragment>
</navigation>